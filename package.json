        "dev:backend-mix": "node ./node_modules/gulp/bin/gulp.js backend-mix-server --dist packages",
        "prepublish": "npm run build:packages",
        "publish": "node ./node_modules/lerna/bin/lerna.js publish -m \"chore(release): publish %s\"",
        "run:install": "node ./node_modules/@notadd/server/install.js",
        "server": "node ./node_modules/@notadd/server/bootstrap.js",
